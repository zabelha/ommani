<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.opeimpacta.ommani">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ProdutoActivity"></activity>
        <activity android:name=".ProdutosActivity" />
        <activity
            android:name=".TelaFaqActivity"
            android:theme="@style/AppTheme.Toolbar" />
        <activity
            android:name=".TelaPerfilActivity"
            android:theme="@style/AppTheme.Toolbar" />
        <activity
            android:name=".TelaSobreActivity"
            android:theme="@style/AppTheme.Toolbar" />
        <activity
            android:name=".TelaConfiguracoesActivity"
            android:theme="@style/AppTheme.Toolbar" />
        <activity android:name=".TelaMassagemActivity" />
        <activity android:name=".TelaYogaActivity" />
        <activity android:name=".TelaCristaisActivity" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DebugActivity" />
        <activity
            android:name=".TelaInicialActivity"
            android:theme="@style/AppTheme.Toolbar" />
    </application>

</manifest>